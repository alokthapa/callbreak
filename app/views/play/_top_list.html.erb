<div class="y2">Top Users List</div>
<table id="topten" cellspacing="5" cellpadding="5" border="1" width="200px">
	<tr>
		<td>
			User Name 
		</td>
		<td>
			Win Difference
		</td>
	</tr>
	<% @users.each do |user|%>
	<tr>
		<td>
			<%= user.name%>
		</td>
		<td>
			<%= user.diff%>
		</td>
	</tr>
	<tr>
		<td colspan="2">
			<span class="y4">The winning difference between your score and the second highest score.</span>
		</td>
	</tr>
	<% end %>
</table>
